<?xml version="1.0"?>
<launch>
  <include file="$(find vehicle)/launch/fs_simulation.launch" >
    <arg name="rviz" value="false" />
  </include>

  <arg name="test-time-limit" default="300" />

  <test test-name="est_pos_test" pkg="test" type="estimated_pos_test.py" time-limit="$(arg test-time-limit)">
    <param name="time-limit" value="$(arg test-time-limit)" />
  </test>
</launch>
